{
	"name": "salcode/fe-cmb2-slider",
	"description": "Create slider using CMB2",
	"license": "Apache-2.0",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Sal Ferrarello",
			"email": "info@ironcodestudio.com"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"webdevstudios/cmb2": "^2.2",
		"johnbillion/extended-cpts": "^3.2"
	},
	"autoload"    : {
		"classmap": [
			"src/"
		],
		"files": [
			"vendor/johnbillion/extended-cpts/extended-cpts.php",
			"vendor/webdevstudios/cmb2/init.php"
		]
	},
	"extra": {
		"installer-paths": {
			"vendor/webdevstudios/cmb2": ["webdevstudios/cmb2"]
		}
	}
}
